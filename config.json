{
  "source": [
    "json/source.json"
  ],
  "platforms": {
    "compose": {
      "transformGroup": "custom/compose",
      "buildPath": "build/android/",
      "files": [
        {
          "destination": "ColorPalette.kt",
          "format": "compose/object",
          "className": "ColorPalette",
          "packageName": "com.webank.design_system.dictionary",
          "filter": "isColor"
        },
        {
          "destination": "BorderRadius.kt",
          "format": "compose/object",
          "className": "BorderRadius",
          "packageName": "com.webank.design_system.dictionary",
          "filter": "isBorderRadius",
          "options": {
            "import": "com.webank.common_ui_compose.theme.Border"
          }
        },
        {
          "destination": "BorderWitdh.kt",
          "format": "compose/object",
          "className": "BorderWidth",
          "packageName": "com.webank.design_system.dictionary",
          "filter": "isBorderWidth",
          "options": {
            "import": "com.webank.common_ui_compose.theme.Border"
          }
        }
      ]
    },
    "ios-swift-separate-enums": {
      "transformGroup": "custom/ios-swift-separate",
      "buildPath": "build/ios-swift/",
      "files": [
        {
          "destination": "Color.swift",
          "format": "ios-swift/any.swift",
          "className": "Color",
          "options": {
            "import": "SwiftUI",
            "objectType": "extension",
            "accessControl": ""
          },
          "filter": "isColor"
        },
        {
          "destination": "BorderWidth.swift",
          "format": "ios-swift/enum.swift",
          "className": "BorderWidth",
          "options": {
            "import": "SwiftUI",
            "accessControl": ""
          },
          "filter": "isBorderWidth"
        },
        {
          "destination": "BorderRadius.swift",
          "format": "ios-swift/enum.swift",
          "className": "BorderRadius",
          "options": {
            "import": "SwiftUI",
            "accessControl": ""
          },
          "filter": "isBorderRadius"
        }
      ]
    }
  }
}
