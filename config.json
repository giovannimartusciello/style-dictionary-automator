    "ios-swift-separate-enums": {
      "transformGroup": "custom/ios-swift-separate",
      "buildPath": "build/ios-swift/",
      "files": [
        {
          "destination": "Color.swift",
          "format": "ios-swift/any.swift",
          "className": "Color",
          "options": {
            "import": "SwiftUI",
            "objectType": "extension",
            "accessControl": ""
          },
          "filter": "isPaletteColor"
        },
        {
          "destination": "DarkColor.swift",
          "format": "ios-swift/enum.swift",
          "className": "DarkColor: GeneratedColor",
          "options": {
            "import": "SwiftUI",
            "accessControl": ""
          },
          "filter": "isDarkColor"
        },
        {
          "destination": "LightColor.swift",
          "format": "ios-swift/enum.swift",
